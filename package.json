{
  "name": "riot-format",
  "version": "0.1.3",
  "description": "a helper library for riotjs to format displays",
  "main": "dist/riot-format.js",
  "scripts": {
    "start": "npm run build && npm run build:example && open examples/index.html",
    "build": "node node_modules/.bin/babel src -o dist/riot-format.js -s --presets es2015 --plugins transform-es2015-modules-umd",
    "build:example": "node node_modules/.bin/riot examples/tags --ext html -m",
    "clean": "rm -r -f dist/*",
    "clean:example": "rm -r -f examples/tags/*.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Joylei/riot-format.git"
  },
  "keywords": [
    "riot",
    "riotjs",
    "format",
    "riot-format",
    "filter"
  ],
  "author": "joylei <leingliu@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Joylei/riot-format/issues"
  },
  "homepage": "https://github.com/Joylei/riot-format#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "riot": "^2.3.12"
  }
}
