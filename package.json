{
  "name": "riot-format",
  "version": "0.1.4",
  "description": "a helper library for riotjs to format displays",
  "main": "dist/riot-format.js",
  "scripts": {
    "start": "npm run clean && npm run build && npm run build:example && open examples/index.html",
    "build": "node node_modules/.bin/rollup -c",
    "build:example": "node node_modules/.bin/rollup --config ./examples/rollup.config.js",
    "clean": "npm run clean:dist && npm run clean:example",
    "clean:dist": "rm -r -f dist/*",
    "clean:example": "rm -r -f examples/bundle.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Joylei/riot-format.git"
  },
  "keywords": [
    "riot",
    "riotjs",
    "format",
    "riot-format",
    "filter"
  ],
  "author": "joylei <leingliu@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Joylei/riot-format/issues"
  },
  "homepage": "https://github.com/Joylei/riot-format#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-rollup": "^1.2.0",
    "riot": "^3.0.1",
    "rollup": "^0.36.4",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-buble": "^0.14.0",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-riot": "^1.0.0",
    "rollup-plugin-uglify": "^1.0.1"
  }
}
